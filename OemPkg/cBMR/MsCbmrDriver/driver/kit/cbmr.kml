        Name="cbmr_driver.efi"
        Source="$(OBJ_PATH)\..\efi_min\$(O)\"
        />
    <BinaryFile
        Name="cbmr_driver_debug.efi"
        Source="$(OBJ_PATH)\..\efi_min_debug\$(O)\"
        />
  </ContentGroup>
</Content>
